{
  "name": "html-parser",
  "version": "1.0.1",
  "description": "A parser that converts html to JSON and JSON to html",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "format": "prettier \"*/**/*.js\" --write",
    "test": "jest --coverage",
    "test:report": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "node_modules/flow-coverage-report/bin/flow-coverage-report.js -i 'src/**/*.js' -x 'src/demo/*.js' -t html -t json -t text"
  },
  "repository": {
    "url": "git@github.com:MaartenGDev/html-parser.git",
    "type": "git"
  },
  "author": "MaartenGDev",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "coveralls": "^2.13.1",
    "flow-bin": "^0.47.0",
    "flow-coverage-report": "^0.3.0",
    "jest": "^20.0.1",
    "prettier": "1.9.2",
    "webpack": "^5.75.0"
  }
}
